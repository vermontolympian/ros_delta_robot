              </dynamics>
              <use_parent_model_frame>0</use_parent_model_frame>
            </axis>
          </joint>

<!-- join union del end effector y la barilla 2 de la pata larga  2-->
          <joint name='con23_lleg_22' type='revolute'>
            <child>con23</child>
            <parent>lleg_2</parent>
            <pose> -0.05 0 0 0 0 0 </pose>
