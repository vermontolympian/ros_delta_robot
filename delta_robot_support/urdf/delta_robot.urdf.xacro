              </dynamics>
              <use_parent_model_frame>0</use_parent_model_frame>
            </axis>
          </joint>

<!-- join union del end effector y la barilla 2 de la pata larga  3-->
          <joint name='con33_lleg_32' type='revolute'>
            <child>con33</child>
            <parent>lleg_3</parent>
            <pose> -0.05 0 0 0 0 0 </pose>
